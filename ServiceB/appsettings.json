{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConsulRegisterOptions": {
    "Address": "http://host.docker.internal:8500", //Consul 客户端地址
